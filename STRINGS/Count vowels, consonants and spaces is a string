int solve(string str) {
  int n=str.length();
  int v=0,c=0,space=0;
  for (int i=0; i<n; i++) {
    str[i] = towlower(str[i]);
  }
  for (int i=0; i<n; i++) {
    if (str[i] == 'a' || str[i] == 'e' || str[i] == 'i' || str[i] == 'o' || str[i] == 'u')
      v++;
    else if (str[i] >= 'a' && str[i] <= 'z')
      c++;
    else if (str[i] == ' ')
      space++;
  }
  cout<<"Vowels are: "<< v << endl;
  cout<<"Constants are: "<< c << endl;
  cout<<"Spaces are: "<< space << endl;
}
